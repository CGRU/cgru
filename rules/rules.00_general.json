{
"root":"rules_root",

"company":"CGRU",
"panel_logo_img":"rules/icons/rules.png",
"-panel_logo_img":"rules_panel.png",
"playlist":"rules/playlist.json",
"cache_time":60,

"news":{
	"limit":99,
	"refresh":60,
	"recent":99
},

"bookmarks":{
	"refresh":300,
	"obsolete_delete_days":10,
	"inactive_delete_days":366
},

"-group_folders_levels":[3,4],

"flags":{
	"hold"        :{"title":"HOLD",        "clr":[111,111,111], "short":"Hl",                          "mode":null,    "tip":"On hold"},
	"question"    :{"title":"QUESTION",    "clr":[210,140,140], "short":"Que",                         "mode":null,    "tip":"Question"},
	"error"       :{"title":"ERROR",       "clr":[255, 55, 55], "short":"Err",             "p_max":50, "mode":null,    "tip":"Error"},
	"ready"       :{"title":"READY",       "clr":[230,230,130], "short":"Rdy",             "p_max": 1, "mode":"stage", "tip":"Ready"},
	"inprogress"  :{"title":"INPROGRESS",  "clr":[210,210,210], "short":"IP",  "p_min":1,  "p_max":90, "mode":"stage", "tip":"In progress"},
	"on_farm"     :{"title":"ONFARM",      "clr":[250,150,100], "short":"OF",  "p_min":20, "p_max":20, "mode":null,    "tip":"On farm"},
	"check"       :{"title":"CHECK",       "clr":[ 77, 77,177], "short":"Chk", "p_min":60, "p_max":90, "mode":"stage", "tip":"To check"},
	"comment"     :{"title":"COMMENT",     "clr":[222,111,222], "short":"Cmm",             "p_max":50, "mode":"stage", "tip":"Comment"},
	"send_ready"  :{"title":"SENDREADY",   "clr":[ 77,177,177], "short":"SR",  "p_min":70, "p_max":90, "mode":"super", "tip":"Ready to send."},
	"sent"        :{"title":"SENT",        "clr":[222,222,191], "short":"SNT", "p_min":80, "p_max":90, "mode":"super", "tip":"Was sent"},
	"omit"        :{"title":"OMIT",        "clr":[ 99, 99, 99], "short":"Om",              "p_max":-1, "mode":"stage", "tip":"Omit"},
	"hqres"       :{"title":"HQRES",       "clr":[100,250,100], "short":"HQR", "p_min":98, "p_max":98, "mode":"stage", "tip":"Render high quality results"},
	"cbb"         :{"title":"CBB",         "clr":[140,145,121], "short":"CBB", "p_min":100,            "mode":"stage", "tip":"Could be better"},
	"done"        :{"title":"DONE",        "clr":[121,155,121], "short":"DON", "p_min":100,            "mode":"stage", "tip":"Done"},
	"closed"      :{"title":"CLOSED",      "clr":[ 11, 55, 11], "short":"CLS", "p_min":100,            "mode":"stage", "tip":"Closed"},
	"aux"         :{"title":"AUX",         "clr":[127,127,127], "short":"AUX",             "p_max":-1, "mode":"stage", "tip":"Auxiliary",
		"new_line":true},
	"precomp"    :{"title":"PRECOMP",  "clr":[ 80, 96,115], "short":"PC", "p_min":10, "p_max":30, "excl":true, "tip":"Precomp exists"}

},

"tags":{
	"anim"    :{"title":"Anim",     "short":"Am",  "tip":"Animation"},
	"cleanup" :{"title":"CleanUp",  "short":"Cl",  "tip":"Cleanup"},
	"comp"    :{"title":"Comp",     "short":"Cp",  "tip":"Compositing"},
	"concept" :{"title":"Concept",  "short":"Cc",  "tip":"Concept"},
	"frlnc"   :{"title":"Freelance","short":"Fl",  "tip":"Freelance"},
	"fur"     :{"title":"Fur",      "short":"Fur", "tip":"Fur, Hair"},
	"fx"      :{"title":"FX",       "short":"FX",  "tip":"FX"},
	"key"     :{"title":"Keying",   "short":"Key", "tip":"Keying"},
	"layout"  :{"title":"Layout",   "short":"Lay", "tip":"Layout"},
	"mimic"   :{"title":"Mimic",    "short":"Mim", "tip":"Mimic"},
	"model"   :{"title":"Model",    "short":"Md",  "tip":"Modeling"},
	"paint"   :{"title":"Paint",    "short":"Pt",  "tip":"Painting"},
	"render"  :{"title":"Render",   "short":"Rn",  "tip":"Rendering"},
	"rig"     :{"title":"Rig",      "short":"Rig", "tip":"Rigging"},
	"roto"    :{"title":"Roto",     "short":"Ro",  "tip":"Rotoscoping"},
	"rnd"     :{"title":"R&D",      "short":"RnD", "tip":"Research and development"},
	"textur"  :{"title":"Textur",   "short":"Tx",  "tip":"Texturing"},
	"track"   :{"title":"Track",    "short":"Tr",  "koeff":0.1, "tip":"Tracking"},
	"cbb"     :{"title":"CBB",      "short":"CBB", "tip":"Could be better"},


	"other"   :{"title":"Other",    "short":"Ot",  "tip":"Something else",
		"new_line":true}
},

"comments":{
	"report" :{"title":"Report" ,"color":[100,200,100]},
	"client" :{"title":"Client" ,"color":[200,200,100]},
	"message":{"title":"Message","color":[150,150,200]}
},

"aux_folders":[".","_","-","tmp","temp","tmp","common"],

"execute":[
	{
		"name":"terminal",
		"label":"Terminal",
		"icon":"icons/terminal.png",
		"title":"Open terminal in this location.",
		"terminal":"\"@PATH@\""
	}
],

"assets":{
	"projects_root":{
		"play_folders":false,
		"show_files":true
	},
	"project":{
		"seek":["[projects_root]/","/"],
		"upload_dir":"_UPLOADS/@DATE@_@USER@",
		"play_folders":false,
		"scripts":["project.js"],
		"template":"rules/assets/templates/project",
		"show_files":true
	},
	"area":{
		"seek":["[project]/SCENE.*","[project]/EPISODE.*","[project]/SHOT.*","[project]/ASSETS.*","[project]/CONCEPT.*","[project]/ANIM.*","[project]/RND.*","[project]/FREELANCE.*"],
		"uploads":{
			"upload":{
				"masks":[".*"],
				"folder":"_UPLOADS/@DATE@_@USER@"
			}
		},
		"-name":"",
		"filter":"sc_FilterShots",
		"scripts":["scenes.js"]
	},
	"scene":{
		"seek":["[area]/"],
		"uploads":{
			"upload":{
				"masks":[".*"],
				"folder":"_UPLOADS/@DATE@_@USER@"
			}
		},
		"filter":"sc_FilterShots",
		"template":"rules/assets/templates/scene",
		"scripts":["scenes.js"]
	},
	"shot":{
		"seek":["[scene]/"],
		"uploads":{
			"upload":{
				"masks":[".*"],
				"folder":"_UPLOADS/@DATE@_@USER@"
			},
			"result":{
				"masks":[".*\\.(zip|rar|7z)$"],
				"folder":"RESULT/ARCHIVES/@DATE@_@USER@",
				"news_title":"upload"
			},
			"dailies":{
				"masks":[".*\\.(mov|mp4|avi)$"],
				"folder":"RESULT/DAILIES"
			}
		},
		"inserted_images":{
			"forms":
			{
				"body":{
					"folder":"REF/BODY"
				},
				"comment":{
					"folder":"REF/COMMENTS"
				}
			}
		},
		"scripts":["shot.js"],
		"-subfolders_status_hide":true,
		"subfolders_dailies_hide":false,
		"post":"shot_Post",
			"":"custom post function",
		"references":{
			"path":["REF"]
		},
		"source":{
			"path":["SRC","SOURCE"]
		},
		"result":{
			"path":["RESULT/JPG","RESULT/PNG","RESULT/DPX","RESULT/TIF","RESULT/EXR","RESULT/FREELANCE","RESULT/ANIM","RESULT/RENDER"]
		},
		"dailies":{
			"path":["RESULT/DAILIES"],
			"folders":{
				"result/freelance":"RESULT/FREELANCE",
				"result/anim":"RESULT/ANIM",
				"render":"RESULT/RENDER"
			}
		},
		"template":"rules/assets/templates/shot",

		"shot_process":[
			{
				"label":"Nk New",
				"title":"Create and open new Nuke scene.",
				"cmd":"/cgru/rules/bin/shot_process -s nuke -r nuke @template@ @fps@ @colorspace@",
				"template":"/cgru/plugins/nuke/_template.nk"
			},{
				"label":"Nk Latest",
				"title":"Open the latest Nuke scene.",
				"cmd":"/cgru/rules/bin/shot_open_latest -s nuke -e .nk -r nuke"
			}
		]
	}
},

"cgru_config":{
	"pathsmap":[
			["/cgru/rules_root/","/rules_root/"],
			["/rules_root/","/cgru/rules_root/"]
		]
},

"OS_unix":{
	"cmdexec":{
		"play_sequence":[
			{"name":"DJV",
				"cmd":"djv_view \"@PATH@\"",
				"tooltip":"Open sequence within 'djv_view'."},
			{"name":"NkCp",
				"cmd":"nukecopy \"@PATH@\"",
				"tooltip":"Copy Nuke Read.\nPaste it in a scene.",
				"submenu":true},
			{"name":"Nuke",
				"cmd":"nukeview \"@PATH@\"",
				"tooltip":"Open Nuke with this sequence Reader.",
				"submenu":true}
		],
		"play_movie":[
			{"name":"VLC",
				"cmd":"vlc \"@PATH@\"",
				"tooltip":"Open with VLC media player."},
			{"name":"Totem",
				"cmd":"totem \"@PATH@\"",
				"tooltip":"Open with Totem video player."}
		]
	}
},

"OS_windows":{
	"cmdexec":{
		"play_sequence":[
			{"name":"Pd++",
				"cmd":"pdplayer --fps=@FPS@ --attach @PATH@",
				"tooltip":"Append sequence in opened PdPlayer."},
			{"name":"PdP",
				"cmd":"pdplayer --fps=@FPS@ @PATH@",
				"tooltip":"Open sequence with PdPlayer."},
			{"name":"NkCp",
				"cmd":"nukecopy @PATH@",
				"tooltip":"Copy Nuke Read.\nPaste it in a scene.",
				"submenu":true},
			{"name":"Nuke",
				"cmd":"nukeview @PATH@",
				"tooltip":"Open Nuke with this sequence Reader.",
				"submenu":true}
		],
		"play_movie":[
			{"name":"QT",
				"cmd":"QuickTimePlayer @PATH@",
				"tooltip":"Open with QuickTime."},
			{"name":"VLC",
				"cmd":"vlc @PATH@",
				"tooltip":"Open with VLC media player."}
		]
	}
},

"thumbnail":{
	"filename":"thumbnail.jpg",
	"colorspace":"auto",
	"cmd_asset":"rules/bin/thumbnail.sh -n 3 -i \"@INPUT@\" -t 600 -o \"@OUTPUT@\"",
	"create_file":"rules/bin/thumbnail.sh -i \"@INPUT@\" -t 600 -o \"@OUTPUT@\""
},

"avcmd":"ffmpeg",
"skipfiles":[".","Thumbs.db"],

"-colorspace":"extension",
"fps":24,
"dailies":
{
	"format":"1280x720",
	"-fps":24,
	"codec":"h264_mid",
	"container":"DEFAULT",
	"colorspace":"auto",
	"template":"dailies",
	"slate":"noslate",
	"-font":"DejaVuSans",
	"naming":"(s)_(a)_(v)_(d)",
	"logo_slate_path":"logo.png",
	"logo_slate_size":20,
	"logo_slate_grav":"SouthEast",
	"logo_frame_path":"logo.png",
	"logo_frame_size":10,
	"logo_frame_grav":"North",

	"activity":"comp",

	"comment_annotation":true,
	"comment_body":false,

	"-imgcmd":"convert",
		"":"Specify image convert and draw command, default is 'convert'",

	"slates":{
		"noslate":      {"name":"No Slate",        "tooltip":"Do not draw first informative frame."},
		"dailies_slate":{"name":"Dailies Slate",   "tooltip":"Common dailies first inforative frame."},
		"slate_noatist":{"name":"Slate No Artist", "tooltip":"Dailies first informative frame with no artist."}
	},
	"templates":{
		"dailies":         {"name":"Dailies",           "tooltip":"Common dailies."},
		"dailies_withlogo":{"name":"Dailies With Logo", "tooltip":"Dailies with a logo image."}
	},

	"colorspaces":{
		"auto":   {"name":"Auto",      "value":"auto",      "tooltip":"Try to read metadata"},
		"ext":    {"name":"Extension", "value":"extension", "tooltip":"By image extension:\nDPX,CIN: cineon\nEXR: linear\nAny Other: sRGB"},
		"linear": {"name":"Linear",    "value":"RGB"},
		"srgb":   {"name":"sRGB",      "value":"sRGB"},
		"log":    {"name":"Cineon",    "value":"Log"}
	},

	"formats":{
		"asis":   {"name":"As Is",   "value":null},
		"pal":    {"name":"PAL",     "value":"720x576x1.09"},
		"pal768": {"name":"PAL 768", "value":"768x576"},
		"1k43":   {"name":"1k 4:3",  "value":"1024x768"},
		"sd360":  {"name":"640x360", "value":"640x360"},
		"sd540":  {"name":"960x540", "value":"960x540"},
		"hd720":  {"name":"HD720",   "value":"1280x720"},
		"hd1080": {"name":"HD1080",  "value":"1920x1080"},
		"2k169":  {"name":"2K 16/9", "value":"2048x1152"},
		"2k235":  {"name":"2K 2.35", "value":"2048x872"}
	},

	"codecs":{
		"photojpg_high": {"name":"JPG Hi",   "tooltip":"PhotoJPG High [mp4]"},
		"photojpg_mid":  {"name":"JPG Mid",  "tooltip":"PhotoJPG Mid [mp4]"},
		"photojpg_low":  {"name":"JPG Lo",   "tooltip":"PhotoJPG Low [mp4]"},
		"h264_high":     {"name":"H264 Hi",  "tooltip":"H.264 High [mp4]"},
		"h264_mid":      {"name":"H264 Mid", "tooltip":"H.264 Mid [mp4]"},
		"h264_low":      {"name":"H264 Lo",  "tooltip":"H.264 Low [mp4]"},
		"theora_high":   {"name":"Th Hi",    "tooltip":"Theora High [ogg]"},
		"theora_mid":    {"name":"Th Mid",   "tooltip":"Theora Mid [ogg]"},
		"theora_low":    {"name":"Th Lo" ,   "tooltip":"Theora Low [ogg]"},
		"dnxhd185":      {"name":"DNxHD" ,   "tooltip":"AVID DNxHD 1920x1080x25:185 [mov]"},
		"prores444":     {"name":"Pro444",   "tooltip":"Apple ProRes444 10Bit [mov]"},
		"prores422":     {"name":"Pro422",   "tooltip":"Apple ProRes422 10Bit [mov]"},
		"copy":          {"name":"COPY",     "tooltip":"Just copy stream [mp4]"}
	},

	"containers":{
		"DEFAULT": {"name":"Default", "tooltip":"Default container:\nProRes, DNxHD: MOV\nTheora: OGG\nAny other: MP4"},
		"mp4": {"name":"MP4", "tooltip":"MPEG-4 Part 14\nIt is a common container."},
		"mov": {"name":"MOV", "tooltip":"Apple QuickTime Movie"},
		"m4v": {"name":"M4V", "tooltip":"Apple 'MP4'"},
		"mxf": {"name":"MXF", "tooltip":"Material eXchange Format"},
		"avi": {"name":"AVI", "tooltip":"Microsoft Audio Video Interleave"}
	},

	"preview":{
		"codec":"h264_mid",
		"args":"-vf scale=640:-1"
	},

	"af_capacity":500
},

"permissions":{
	"default_groups":["admins","users"]
},

"inserted_images":{
	"size_make_thumbnail":200000,
	"thumbnail_cmd":"rules/bin/convert -verbose \"@INPUT@\" -quality 85% -thumbnail 1280 \"@OUTPUT@\""
},

"cut":{
	"input":"RESULT/ANIM,RESULT/PNG,RESULT/JPG,RESULT/TIF,RESULT/DPX,RESULT/EXR",
	"output":"COMMON/RESULT/JPG",
	"af_capacity":500,
	"af_maxtasks":10,
	"af_perhost":-1,
	"af_pertask":100,
	"af_maxruntime":120
},

"tmpfio":{
	"template":"rules/assets/tmpfio"
},

"put":{
	"input":"RESULT/PNG,RESULT/TIF,RESULT/DPX,RESULT/EXR,RESULT/MOV",
	"dest":"OUT",
	"-dest":"/incoming",
	"cmd":"python3 /cgru/utilities/put.py",
	"-ftp":{
		"host":"localhost",
		"user":"",
		"pass":""
	},
	"post_delete":false,
	"af_service":"generic",
	"-af_service":"ftp",
	"af_parser":"generic",
	"af_capacity":500,
	"af_maxtasks":5,
	"af_perhost":1
},

"archive":{
	"types":{
		"zip": {"name":"ZIP", "tooltip":"Fastest, simplest, no special programs needed, recommended"},
		"rar": {"name":"RAR", "tooltip":"For WinRAR lowers"},
		"7z" : {"name":"7Z",  "tooltip":"Slowest, but small archive size"}
	},
	"default":"zip",
	"cmd":"python3 /cgru/utilities/arch.py",
	"cmd_x":"python3 /cgru/utilities/arch_x.py",
	"dest":"",
	"af_service":"arch",
	"af_parser":"generic",
	"af_capacity":500,
	"af_maxtasks":5
},

"walk":{
	"cmd":"python3 /cgru/rules/bin/walk.py",
	"af_service":"walk",
	"af_parser":"generic",
	"af_capacity":100
},

"checksum":{
	"md5":{
		"cmd":"python3 /cgru/utilities/checksum.py",
		"name":"MD5",
		"af_service":"checksum",
		"af_parser":"generic",
		"af_capacity":500
	}
},

"":""

}
